<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/userListFragment">

    <fragment
        android:id="@+id/userListFragment"
        android:name="com.example.retrofitmvvmjava.View.UserListFragment"
        android:label="fragment_user_list"
        tools:layout="@layout/fragment_user_list" >
        <action
            android:id="@+id/action_userListFragment_to_userDetailsView"
            app:destination="@id/userDetailsView" />
    </fragment>
    <fragment
        android:id="@+id/userDetailsView"
        android:name="com.example.retrofitmvvmjava.View.UserDetailsView"
        android:label="fragment_user_details_view"
        tools:layout="@layout/fragment_user_details_view" />
</navigation>